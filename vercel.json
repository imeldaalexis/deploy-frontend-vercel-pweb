{
  "version": 2,
  "builds": [
    { 
      "src": "index.js", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "index.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; img-src 'self' data: https://deploy-frontend-vercel-pweb.vercel.app"
        }
      ]
    }
  ]
}
